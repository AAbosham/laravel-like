{
    "name": "aabosham/laravel-like",
    "description": "Create like for any models for laravel framework",
    "keywords": [
        "aabosham",
        "laravel",
        "like"
    ],
    "homepage": "https://github.com/aabosham/laravel-like",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abobaker",
            "email": "albaakr@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Aabosham\\Likeable\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aabosham\\Likeable\\LikeableServiceProvider"
            ],
            "aliases": {
                "Likeable": "Aabosham\\Likeable\\LikeableFacade"
            }
        }
    }
}
